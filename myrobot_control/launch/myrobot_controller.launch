<launch>

    <rosparam file="$(find myrobot_description)/launch/myrobot_control.yaml" command="load"/>
    <node args="Rev1_position_controller Rev2_position_controller joint_state_controller " name="controller_spawner" ns="myrobot" output="screen" pkg="controller_manager" respawn="false" type="spawner"/>
    <node name="robot_state_publisher" output="screen" pkg="robot_state_publisher" respawn="false" type="robot_state_publisher">
        <remap from="/joint_states" to="/er_robot/joint_states"/>
    </node>

</launch>